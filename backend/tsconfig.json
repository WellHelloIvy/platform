{
  "compilerOptions": {
    "alwaysStrict": true,
    "strict": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "es6",
    "allowJs": true,
    "rootDir": "./",
    "outDir": "./build",
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "build",
    "node_nodules"
  ]
}
